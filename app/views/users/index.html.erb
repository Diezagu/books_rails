<h1>Authors</h1>
<%= link_to 'Log out', destroy_user_session_path, method: :delete %>
<%= link_to 'Edit', edit_user_registration_path, method: :get %>

<% @users.each do |user| %>
  <p> <%= user.name %> </p>
  <p> <%= user.age %> </p>
  <%= link_to 'Show', user_path(user) %>
<% end %>
<div>
  <%= link_to 'Download Report', users_path(format: :csv) %>
</div>

