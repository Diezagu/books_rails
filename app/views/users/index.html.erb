<h1>Authors</h1>
<hr/>
<%= link_to 'Create', new_user_path %>
<% @users.each do |user| %>
	<p> <%= user.name %> </p>
	<p> <%= user.age %> </p>

	<%= link_to 'Show', user_path(user) %>
	<%= link_to 'Edit', edit_user_path(user) %>
	<%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are u sure?' } %>
	<hr>
<% end %>
<%= link_to 'Download Report', users_path(format: :csv) %>

